<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Cor de fundo -->
    <solid android:color="#FFFFFF" />

    <!-- Borda -->
    <stroke
        android:width="0dp"
        android:color="#FFFFFF" />

    <!-- Cantos arredondados -->
    <corners android:radius="25dp" /> <!-- Radius de 8dp -->

    <!-- Padding interno -->
    <padding
        android:left="6dp"
        android:top="6dp"
        android:right="6dp"
        android:bottom="6dp" /> <!-- Padding interno de 8dp -->
</shape>
